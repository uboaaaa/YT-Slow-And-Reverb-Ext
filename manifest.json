{
    "manifest_version": 2,
    "name": "Slow and Reverb Controller",
    "version": "1.0",
    "description": "Allows slowing and adding reverb to audio on the page.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icons/icon16.svg",
      "48": "icons/icon48.svg",
      "128": "icons/icon128.svg"
    }
  }
  